5."]}, {"N": 20, "V": [0, "", ["Senegal"]]}]}, {"CN": 0, "CV": [{"N": 3, "V": [1, 0, 2]}], "CI": "Senegalia - Phulai Tree/Phulai Tree - Bark.jpg"}, {"CN": 3, "CV": [{"N": 3, "V": [1, 0, 3]}, {"N": 21, "V": [0, "", "Apr-May, Jul-Aug"]}], "CI": "Senegalia - Phulai Tree/Phulai Tree - Flower.jpg"}, {"CN": 4, "CV": [{"N": 23, "V": [0, "", "pod"]}], "CI": "Senegalia - Phulai Tree/Phulai Tree - Fruit.jpg"}, {"CN": 8, "CV": [{"N": 32, "V": [1, 4, 2]}, {"N": 28, "V": [0, "", "2-3 pairs"]}, {"N": 13, "V": [0, "", "3-5 pairs"]}], "CI": "Senegalia - Phulai Tree/Phulai Tree - Leaf.jpg"}, {"CN": 10, "CV": [{"N": 23, "V": [0, "", "straight"]}, {"N": 3, "V": [1, 0, 2]}, {"N": 11, "V": [0, "", "in pairs"]}], "CI": "Senegalia - Phulai Tree/Phulai Tree - Spine.jpg"}], "mapinfo": [{"lat": "12.9483584607", "long": "77.5862173562"}, {"lat": "12.9487880657", "long": "77.5863735799"}], "mapregion": [["12.9483584607", "77.5862173562"], ["12.9487880657", "77.5863735799"]]}
